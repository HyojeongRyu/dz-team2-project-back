<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dz.factory.management.mapper.CodeMapper">
	<select id="selCodeAll" resultType="Code">
		select * from common_code
	</select>
	
	<select id="selManageCodeAll" resultType="ManageCode">
		select * from management_code
	</select>
	
	<insert id="insertCode" parameterType="Code">
		insert into common_code (code,name,management_code_id) 
		values(#{code},#{name},#{management_code_id})
	</insert>
	
	<insert id="insertManageCode" parameterType="ManageCode">
		insert into management_code (management_name,management_code) 
		values(#{management_name},#{management_code})
	</insert>


</mapper>