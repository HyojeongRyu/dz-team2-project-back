package com.dz.factory.service;

import java.util.ArrayList;

import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

import com.dz.factory.mapper.StorageMapper;

import lombok.RequiredArgsConstructor;

@RequiredArgsConstructor
@Service
public class StroageService {

	private final StorageMapper storageMapper;
	
	@Transactional
	public void insertStorage(ArrayList<String> storage) {
		for(String storage_name : storage) {
			storageMapper.insertStorage(storage_name);
		}
	}
	
}
