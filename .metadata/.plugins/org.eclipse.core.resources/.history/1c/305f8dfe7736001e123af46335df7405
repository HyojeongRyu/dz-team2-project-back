<mapper
	namespace="com.dz.factory.mapper.management.PartnerMapper">
	<insert id="insertPartner" parameterType="Partner">
		INSERT INTO partner
		(bizNum, name, company_id,emp_name, ph_num, address,email)
		VALUES (#{bizNum},#{name},1,#{emp_name},#{ph_num},#{address},#{email})
	</insert>
	
	<select id="seletAllPartner" parameterType="Partner">
		select * from partner
	</select>
</mapper>