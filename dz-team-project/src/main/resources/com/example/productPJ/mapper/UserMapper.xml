<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dz.factory.mapper.ProductMapper.java">

    <insert id="insertUser" parameterType="com.example.productPJ.domain.user.User">
        INSERT INTO User (userId, password, name, gender,createdate)
        VALUES (#{userId}, #{password}, #{name}, #{gender} , #{createDate})
    </insert>
    
    <select id="selectUser" parameterType="String" resultType="com.example.productPJ.domain.user.User">
        SELECT * FROM User WHERE username = #{username}
    </select>
    
</mapper>