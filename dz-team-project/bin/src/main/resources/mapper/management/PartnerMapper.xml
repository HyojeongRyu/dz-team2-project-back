<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.dz.factory.mapper.management.PartnerMapper">
	<insert id="insertPartner" parameterType="Partner">
		INSERT INTO partner
		(bizNum, name, company_id,emp_name, ph_num, address,email)
		VALUES
		(#{bizNum},#{name},1,#{emp_name},#{ph_num},#{address},#{email})
	</insert>

	<select id="seletAllPartner" resultType="Partner">
		select * from partner
	</select>

	<delete id="deletePartner" parameterType="int">
		DELETE from
		partner
		WHERE partner_id = ${id}
	</delete>
</mapper>